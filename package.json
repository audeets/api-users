{
  "name": "audeets-api-users",
  "version": "1.0.0",
  "private": false,
  "author": "Benoit Quette",
  "main": "app.js",
  "scripts": {
    "start": "node --env-file=.env ./bin/www",
    "dev": "node --trace-deprecation --watch --env-file=.env ./bin/www"
